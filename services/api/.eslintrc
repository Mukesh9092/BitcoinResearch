{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "arrow-body-style": ["error", "always"],
    "arrow-parens": ["error", "always"],
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-unused-vars": "warn",
    "react/prop-types": 0
  }
}
