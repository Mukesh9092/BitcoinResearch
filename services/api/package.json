    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.13",
    "apollo-server-core": "^2.9.13",
    "body-parser": "^1.19.0",
    "ccxt": "^1.20.48",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-cli": "^3.0.14",
    "graphql-import": "^0.7.1",
    "graphql-tag": "^2.10.1",
    "graphql-yoga": "^1.18.3",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "prisma-binding": "^2.3.16"
  },
