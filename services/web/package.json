{
  "name": "flush-requires-boilerplate-webpack-chunknames",
  "description": "A boilerplate showing how to achieve Universal Code-Splitting and Universal HMR with react-universal-component, webpack-flush-chunks and extract-css-chunks-webpack-plugin",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "James Gillmore <james@faceyspacey.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "clean": "rimraf dist",
    "lint": "eslint",
    "fix": "eslint --fix",
    "prettier": "prettier --write ./**/*.jsx ./**/*.js",
    "test": "jest",
    "dev": "npm run start",
    "start": "npm run clean; NODE_ENV=development babel-node index.js",
    "start:prod": "NODE_ENV=production babel-node index.js"
  },
  "lint-staged": {
    "./**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^1.35.7",
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-client": "^2.2.7",
    "apollo-link-http": "^1.5.3",
    "colors": "^1.2.1",
    "cookie-parser": "^1.4.3",
    "cross-fetch": "^2.1.0",
    "emotion": "^9.0.2",
    "express": "^4.16.3",
    "express-no-favicons": "^0.0.1",
    "extract-css-chunks-webpack-plugin": "^2.0.18",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "history": "^4.7.2",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "normalize.css": "^8.0.0",
    "p-throttle": "^1.1.0",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "travis-github-status": "^1.6.3",
    "unhandled-error": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-3": "^7.0.0-beta.40",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-universal-import": "^1.4.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "isomorphic-style-loader": "^4.0.0",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "postcss-loader": "^2.1.2",
    "prettier": "^1.11.1",
    "react-hot-loader": "^4.0.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-node-externals": "^1.6.0",
    "write-file-webpack-plugin": "^4.2.0"
  }
}
