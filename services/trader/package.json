{
  "name": "project-seed-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean; yarn build:tsc; yarn build:parcel; yarn build:uml",
    "build:tsc": "tsc -p .; rm -rf lib/*.js lib/*.jsx",
    "build:parcel": "parcel build -t node -d lib src/index.ts",
    "build:uml": "tplant --input src/**/*.ts src/**/*.tsx --output lib/plantuml.svg",
    "clean": "rm -rf lib/*; rm -rf .cache/*; mkdir -p .cache; mkdir -p lib",
    "dev": "nodemon -V -d 2.5 -x ./node_modules/.bin/ts-node src/index.ts",
    "lint": "yarn lint:typescript; yarn lint:eslint; yarn lint:prettier",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:fix": "yarn lint:fix:prettier; yarn lint:fix:eslint",
    "lint:fix:eslint": "eslint 'src/**/*.ts' --fix",
    "lint:fix:prettier": "prettier --write 'src/**/*.ts'",
    "lint:prettier": "prettier --check 'src/**/*.ts'",
    "lint:typescript": "tsc -p . --noEmit",
    "start": "node lib/index.js",
    "test": "jest",
    "types": "npx typesync; yarn",
    "watch:parcel": "parcel watch -t node -d lib src/index.ts",
    "watch:test": "jest --watchAll"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.steps.ts",
      "**/*.test.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/babel__core": "^7.1.3",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.6",
    "@types/cors": "^2.8.6",
    "@types/eslint": "^6.1.3",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.14",
    "@types/parcel-bundler": "^1.12.1",
    "@types/prettier": "^1.19.0",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-plugin-transform-typescript-metadata": "^0.2.2",
    "bumpit": "^1.0.2",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-cucumber": "^2.0.11",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-babel-typescript": "^1.0.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "sinon": "^7.5.0",
    "tplant": "^2.3.3",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "ccxt": "^1.20.44",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.9.1"
  }
}
