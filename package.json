{
  "name": "test-next",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint",
    "lint": "npm run eslint .",
    "lint:fix": "npm run eslint --fix .",
    "precommit": "lint-staged",
    "prettier": "prettier",
    "prettier:all": "find . -name '*.js' | sed '/.next/d' | sed '/node_modules/d' | xargs ./node_modules/.bin/prettier --write"
  },
  "lint-staged": {
    "gitDir": "./",
    "linters": {
      "*.js": ["prettier --write", "git add"]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Industrial/test-next.js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Industrial/test-next.js/issues"
  },
  "homepage": "https://github.com/Industrial/test-next.js#readme",
  "dependencies": {
    "eslint": "^4.6.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "prettier": "^1.6.1"
  }
}
