{
  "name": "test-next",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint",
    "lint": "npm run eslint .",
    "lint:fix": "npm run eslint --fix .",
    "precommit": "lint-staged",
    "prettier": "prettier",
    "prettier:all": "find . -name '*.js' | sed '/.next/d' | sed '/node_modules/d' | xargs ./node_modules/.bin/prettier --write",
    "typescript-build": "cd lib; tsc",
    "typescript-watch": "cd lib; tsc -w"
  },
  "lint-staged": {
    "gitDir": "./",
    "linters": {
      "*.js": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Industrial/test-next.js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Industrial/test-next.js/issues"
  },
  "homepage": "https://github.com/Industrial/test-next.js#readme",
  "dependencies": {
    "@ljharb/eslint-config": "^12.2.1",
    "@types/autobahn": "^0.9.38",
    "@types/body-parser": "^1.16.8",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.0.39",
    "@types/lodash": "^4.14.86",
    "@types/morgan": "^1.7.35",
    "@types/next": "^2.4.6",
    "@types/node": "^8.0.53",
    "@types/passport": "^0.4.1",
    "@types/passport-local": "^1.0.32",
    "@types/react": "^16.0.25",
    "@types/redux": "^3.6.0",
    "babel-eslint": "^8.0.2",
    "eslint": "^4.6.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prettier": "^1.6.1",
    "typescript": "^2.6.1"
  }
}
